---
layout: null
---
<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9"
        xmlns:image="http://www.google.com/schemas/sitemap-image/1.1"
        xmlns:mobile="http://www.google.com/schemas/sitemap-mobile/1.0">
  
  <!-- Página Principal -->
  <url>
    <loc>{{ site.url }}/</loc>
    <lastmod>{{ site.time | date_to_xmlschema }}</lastmod>
    <changefreq>weekly</changefreq>
    <priority>1.0</priority>
    <image:image>
      <image:loc>{{ site.url }}/LOGO%20LAGAX.jpg</image:loc>
      <image:title>Logo LAGAX</image:title>
    </image:image>
    <image:image>
      <image:loc>{{ site.url }}/LOGISTICA%20IMPO%20EXPORT.png</image:loc>
      <image:title>Logística Impo Export</image:title>
    </image:image>
    <image:image>
      <image:loc>{{ site.url }}/DISTRIBUCION.png</image:loc>
      <image:title>Distribución de Productos</image:title>
    </image:image>
    <image:image>
      <image:loc>{{ site.url }}/ASESORIAS.png</image:loc>
      <image:title>Asesorías Comerciales</image:title>
    </image:image>
    <image:image>
      <image:loc>{{ site.url }}/SOLDADURA%20PAILERIA.png</image:loc>
      <image:title>Soldadura y Pailería</image:title>
    </image:image>
    <image:image>
      <image:loc>{{ site.url }}/SOFTWARE.jpg</image:loc>
      <image:title>Soluciones de Software</image:title>
    </image:image>
    <image:image>
      <image:loc>{{ site.url }}/FINANCIERA.png</image:loc>
      <image:title>Soluciones Financieras</image:title>
    </image:image>
  </url>

  <!-- Páginas Adicionales -->
  {% for doc in site.pages %}
    {% if doc.url != "/sitemap.xml" and doc.url != "/404.html" %}
      <url>
        <loc>{{ site.url }}{{ doc.url | replace: 'index.html', '' }}</loc>
        <lastmod>{{ doc.mtime | date_to_xmlschema }}</lastmod>
        <changefreq>monthly</changefreq>
        <priority>0.8</priority>
      </url>
    {% endif %}
  {% endfor %}

</urlset>
